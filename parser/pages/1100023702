	var x = document.getElementsByTagName("META");
	    var txt = "";
	    var i;
	    for (i = 0; i < x.length; i++) {
	        if (x[i].name=="ST.PlatformAPIKey")
	        {
	          var platformKey = x[i].content;
	         }
 	        if (x[i].name=="ST.customerId")
 	        {
 	          var customerKey = x[i].content;
 	         }
	    }
	
	var substatus = "valid";
	if(customerKey == "") 
	{
		substatus = "not subscribed";
	}
	


	var refer = document.referrer;
	 var refcheck = refer.indexOf("name");
	 

 	if(refcheck > 10) {
 	_$tarQ.push(['track', {datatype: 'DATABASE', title: 'YBIO', itemtype: 'RESULT_CLICK'}]);
 	}
	 
	 if(substatus == "valid") {
	_$tarQ.push(['track', {datatype: 'DATABASE', title: 'YBIO', itemtype: 'RECORD_VIEW'}]);
	}


});</script>
  
  <script type="text/javascript">
    (function () {
      var stars = document.createElement('script');
      stars.type = 'text/javascript';
      stars.async = true;
      stars.src = 'https://receiver.star.saas.semcs.net/star.js';
      var s = document.getElementsByTagName('script')[0];
      s.parentNode.insertBefore(stars, s);
  })();
  </script>
  
